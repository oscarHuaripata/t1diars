
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table">
    <thead>
        <tr>
            <th>id</th>
            <th>nombre</th>
            <th>apellido</th>
            <th>fecha</th>
            <th>dni</th>
            <th>genero</th>
            <th>ciuda</th>
            <th>direccion</th>
            <th>correo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.nombre</td>
                <td>@item.apellido</td>
                <td>@item.fechaNac</td>
                <td>@item.DNI</td>
                <td>@item.genero</td>
                <td>@item.categoria.nombre</td>
                <td>@item.direccion</td>
                <td>@item.correo</td>
            </tr>
        }
    </tbody>
</table>